<style>

    .custom-form {
    display: flex;
    flex-direction: column;
    gap: 10px;
    }

    .custom-input {
    width: 100%; 
    height: 40px; 
    padding: 10px;
    border: 1px solid #ced4da; 
    border-radius: 4px; 
    font-size: 16px;
    box-shadow: inset 0 1px 2px rgba(0, 0, 0, 0.075); 
    }

    .btn-small {
    width: auto; 
    max-width: 100%; 
    }

    .coupon-message {
    margin-top: 10px;
    text-align: center; 
    }
    .btn-custom-success {
    background-color:#cc9966; 
    border: none;
    border-radius: 4px;
    color: #fff;
    padding: 8px 16px; 
    font-size: 14px; 
    cursor: pointer;
    transition: background-color 0.3s ease, box-shadow 0.3s ease;
    }

    .btn-custom-success:hover {
    background-color: #b1ca7c;
    box-shadow: 0px 4px 6px rgba(0, 0, 0, 0.2); 
    }

    .summary-total del {
            color: #d9534f; 
            text-decoration: line-through;
        }

        .summary-total span {
            color: #5bc0de; 
        }
        .btn {
        padding: 8px 16px;
        font-size: 14px;
        border-radius: 4px;
        cursor: pointer;
    }

    .btn-custom-danger {
        background-color: rgb(242, 32, 46);
        color: white;
        border: none;
    }

    .btn-small {
        padding: 6px 12px;
        font-size: 12px;
    }

    /* #remove-coupon {
        margin-top: 10px; 
    } 
    */
    .coupon-list {
    margin-top: 15px;
    padding: 10px;
    border: 1px solid #ccc;
    border-radius: 5px;
    background-color: #f9f9f9;
}

.coupon-list ul {
    list-style: none;
    padding: 0;
}

.coupon-list li {
    margin-bottom: 10px;
    padding: 10px;
    border: 1px solid #e0e0e0;
    border-radius: 5px;
    background-color: #ffffff;
}

.btn-custom-info {
    background-color: #17a2b8;
    color: white;
}

.btn-custom-info:hover {
    background-color: #138496;
}


</style>
